{
  "name": "parsimony",
  "version": "1.0.0",
  "private": true,
  "license": "UNLICENSED",
  "description": "Parsimony Behavior Tracking",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "jest",
    "build": "cd packages/app/ && yarn build",
    "deploy:prd": "cd deploy-scripts/ && python3 deploy.py && cd ../",
    "start:server": "cd packages/server/ && yarn start",
    "start:app": "cd packages/app/ && yarn start",
    "build:types": "cd packages/server/ && yarn build:types",
    "start": "concurrently \"yarn start:server\" \"yarn start:app\"",
    "cypress:open": "cd packages/e2e/ && yarn cypress:open",
    "cypress:run": "cd packages/e2e/ && yarn cypress:run",
    "updatePublishedLibsVersionNumber": "yarn lerna version minor --yes && git add -A",
    "prepare": "husky install"
  },
  "devDependencies": {
    "concurrently": "^7.2.2",
    "lerna": "^6.6.1",
    "process": "^0.11.10",
    "husky": "^8.0.0"
  },
  "resolutions": {},
  "keywords": [],
  "dependencies": {
    "@yarnpkg/plugin-workspace-tools": "4.0.0-rc.40"
  }
}
